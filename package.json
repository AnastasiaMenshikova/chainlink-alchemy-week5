{
  "name": "chainlink-dynamic-nft-alchemy",
  "version": "0.1.0-alpha",
  "private":"true",

  "scripts": {
    "lint": "solhint 'contracts/*.sol'",
    "lint:fix": "solhint 'contracts/**/*.sol' --fix",
    "format": "prettier --write .",
    "compile": "hardhat compile",
    "deploy": "hardhat deploy --network rinkeby",
    "test": "hardhat test",
    "coverage": "hardhat coverage",
    "verify": "hardhat etherscan-verify --network rinkeby",
    "slither": "slither . --solc-remaps '@openzeppelin=node_modules/@openzeppelin' --exclude naming-convention,external-function,low-level-calls"
  },

  "devDependencies": {
    "@nomiclabs/hardhat-ethers": "npm:hardhat-deploy-ethers",
    "@nomiclabs/hardhat-etherscan": "^3.1.0",
    "@nomiclabs/hardhat-waffle": "^2.0.3",
    "chai": "^4.3.6",
    "ethereum-waffle": "^3.4.4",
    "ethers": "^5.6.8",
    "hardhat": "^2.9.9",
    "hardhat-abi-exporter": "^2.10.0",
    "hardhat-deploy": "^0.11.4",
    "hardhat-gas-reporter": "^1.0.7",
    "prettier": "^2.7.1",
    "solidity-coverage": "^0.7.21"
  },

  "dependencies": {
    "@chainlink/contracts": "^0.4.1",
    "@ethersproject/bignumber": "^5.5.0",
    "@openzeppelin/contracts": "^4.5.0",
    "base64-sol": "^1.1.0",
    "dotenv": "^16.0.1",
    "fs": "^0.0.1-security",
    "path": "^0.12.7",
    "prettier-plugin-solidity": "^1.0.0-beta.19",
    "solhint": "^3.3.7"
  },

  "mocha": {
    "timeout": 10000000
  }
}
